<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="application">

    <!--GWT -->

    <!-- Inherit the core Web Toolkit stuff.   -->
    <inherits name='com.google.gwt.user.User'/>
    <!-- Inherit the UiBinder module.                -->
    <inherits name="com.google.gwt.uibinder.UiBinder"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.application.client.code.java.Application'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/ApplicationService' class='com.application.server.service.impl.application.ApplicationServiceImpl'/>
    <servlet path='/AuthorizationService' class='com.application.server.service.impl.authorization.AuthorizationServiceImpl'/>
    <source path="client"/>
    <source path="server"/>
    <source path="navigale"/>


</module>